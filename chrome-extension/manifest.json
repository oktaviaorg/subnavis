{
  "manifest_version": 3,
  "name": "TAO Wallet Tracker",
  "version": "1.0",
  "description": "Track your Bittensor TAO wallets",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}
